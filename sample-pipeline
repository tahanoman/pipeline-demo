# Example pipeline for a Node.js project demo without dependencies
trigger:
- main

pool:
  name: Default
  demands:
    - agent.name -equals TAHANOMAN

steps:
- checkout: self
  displayName: "Checkout Repository"

- script: |
    echo "Initializing the environment..."
    node -v
    npm -v
    echo "Environment initialized successfully."
  displayName: "Verify Environment"

- script: |
    echo "Running demo tasks..."
    echo "This is a demo pipeline with no dependencies to install."
    echo "Pipeline executed successfully!"
  displayName: "Run Demo Tasks"
